{
  "fields": [{
      "name": "institution",
      "constraints": {
        "required": true,
        "unique": false,
        "max-length": 6,
        "pattern": "AG[0-9]{4}"
      }
    },
    {
      "name": "member",
      "constraints": {
        "required": true,
        "pattern": "AG[0-9]{4}"
      }
    },
    {
      "name": "source",
      "constraints": {
        "required": true,
        "pattern": "W[0-9]{4}"
      }
    },
    {
      "name": "page",
      "constraints": {
        "required": false
      }
    }, {
      "name": "note",
      "description": "structural entry for internal notes",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "created",
      "description": "structural entry creation date of entry",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "created_by",
      "description": "structural entry acronym of project member creating entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    },
    {
      "name": "last_modified",
      "description": "structural entry last modification date",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "last_modified_by",
      "description": "structural entry acronym of project member last modifying entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    }
  ],
  "primaryKey": ["institution", "member"],
  "missingValues": ["", "NULL"],
  "foreignKeys": [
    {
      "fields": "institution",
      "reference": {
        "resource": "institution",
        "fields": "inst_id"
      }
    },
    {
      "fields": "member",
      "reference": {
        "resource": "agent",
        "fields": "agent_id"
      }
    },
    {
      "fields": "source",
      "reference": {
        "resource": "work",
        "fields": "work_id"
      }
    }
  ]
}
