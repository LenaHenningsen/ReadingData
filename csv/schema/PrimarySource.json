{
  "fields": [{
      "name": "prim_source_id",
      "constraints": {
        "required": true,
        "unique": false,
        "max-length": 5,
        "pattern": "W[0-9]{4}"
      }
    },
    {
      "name": "title",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "subtitle",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "title_lang",
      "constraints": {
        "required": true,
        "min-length": 2
      }
    },
    {
      "name": "genre",
      "constraints": {
        "required": true,
        "pattern": "G[0-9]{2}"
      }
    },
    {
      "name": "publication_place",
      "constraints": {
        "required": false,
        "pattern": "(^http|SP[0-9]{4}|^$)"
      }
    },
    {
      "name": "publication_date",
      "constraints": {
        "required": false,
        "pattern": "(\\[?(-?[0-9X]{4}[\\.~\\?%,]?\\.?){1,2}\\]?|\\d{4}-\\d{2}-\\d{2})"
      }
    },
    {
      "name": "publishing_house",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "first_chin_edition",
      "constraints": {
        "required": false,
        "pattern": "(\\[?(-?[0-9X]{4}[\\.~\\?%,]?\\.?){1,2}\\]?|\\d{4}-\\d{2}-\\d{2})"
      }
    }, {
      "name": "neibu",
      "constraints": {
        "required": false,
        "pattern": "(^$|yes|no)"
      }
    }, {
      "name": "source",
      "constraints": {
        "required": false
      }
    }, {
      "name": "serial",
      "constraints": {
        "required": false
      }
    }, {
      "name": "page",
      "constraints": {
        "required": false
      }
    }, {
      "name": "note",
      "description": "structural entry for internal notes",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "created",
      "description": "structural entry creation date of entry",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "created_by",
      "description": "structural entry acronym of project member creating entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    },
    {
      "name": "last_modified",
      "description": "structural entry last modification date",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "last_modified_by",
      "description": "structural entry acronym of project member last modifying entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    }
  ],
  "primaryKey": ["prim_source_id", "title_lang"],
  "missingValues": [""],
  "foreignKeys": [
    {
      "fields": "prim_source_id",
      "reference": {
        "resource": "work",
        "fields": "work_id"
      }
    },
    {
      "fields": "genre",
      "reference": {
        "resource": "genre",
        "fields": "genre_id"
      }
    },
    {
      "fields": "title_lang",
      "reference": {
        "resource": "iso-lang-code",
        "fields": "iso_code"
      }
    }
  ]
}
